{
 "IO": {
  "model_dir": "model/"
 },
 "HyperParam": {
  "batch_size": 8,
  "lr": 1e-04,
  "epoch": 8,
  "accumulation_step": 4
 },
 "Loss": {
  "bias": [1, 2]
 },
 "NetWork": {
  "pretrained_model": "allenai/biomed_roberta_base",
  "dropout_prob": 0.3,
  "fix_pretrained": true
 },
 "Dataset": {
  "tokenizer_path": "allenai/biomed_roberta_base",
  "training_set": "rsc/training_set.json",
  "test_set": "rsc/test_set.json",
  "text_key": "abstract",
  "label_key": "label",
  "cats": ["Case_Report", "Treatment", "Prevention", "Diagnosis", "Transmission", "Epidemic_Forecasting", "Mechanism"]
 },
 "Predict": {
  "positive_threshold": 0.5
 }
}